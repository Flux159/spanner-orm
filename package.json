{
  "name": "spanner-orm",
  "version": "0.0.2",
  "description": "A simple ORM for Google Cloud Spanner and Postgres",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "spanner-orm-cli": "./dist/cli.js"
  },
